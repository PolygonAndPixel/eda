    <grade_frac>1</grade_frac>
    <n_live>250</n_live>
    <feedback>0</feedback>
    <max_dead>-1</max_dead>
    <boost_posterior>0.0</boost_posterior>
    <num_repeats>-1</num_repeats>
    <posteriors>false</posteriors>
    <equals>false</equals>
    <cluster_posteriors>false</cluster_posteriors>
    <do_clustering>true</do_clustering>
