<?xml version="1.0" encoding="UTF-8"?>
<Minimizer
xmlns="https://www.w3schools.com"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="MultiNestSchema.xsd">
    <name>multi</name>
    <tolerance>1e-3</tolerance>
    <max_iter>0</max_iter>
    <seed>1025</seed>
